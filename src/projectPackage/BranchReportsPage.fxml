<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectPackage.BranchReportsPageController">
   <children>
    <Button fx:id="generateBarChartButton" layoutX="450.0" layoutY="70.0" mnemonicParsing="false" onAction="#generateBarChart" text="Generate Bar Chart" />
      <ComboBox fx:id="selectBranchComboBox" editable="true" layoutX="29.0" layoutY="70.0" onAction="#selectBranch" prefWidth="150.0" promptText="Select Branch..." />
      <Label layoutX="273.0" layoutY="23.0" text="Branch Reports">
         <font>
            <Font name="Lucida Grande" size="13.0" />
         </font>
      </Label>
      <Label layoutX="141.0" layoutY="23.0" text="Executive Home -&gt; ">
         <font>
            <Font name="Lucida Grande" size="13.0" />
         </font>
      </Label>
      <Button fx:id="backToHomeButton" layoutX="14.0" layoutY="17.0" mnemonicParsing="false" onAction="#backToExecutiveHome" text="Back to home">
         <graphic>
            <ImageView fitHeight="22.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Home-Icon.jpeg" />
               </image></ImageView>
         </graphic></Button>
   </children>
</AnchorPane>
